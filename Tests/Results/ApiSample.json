        "FilterPattern": "-\"*** \"",
        "LogGroupName": {
          "Ref": "MyFunctionLogGroup"
        },
        "RoleArn": {
          "Fn::If": [
            "LambdaSharpLoggingStreamRoleIsImported",
            {
              "Fn::ImportValue": {
                "Fn::Sub": [
